{
  "$schema": "https://dl.viam.dev/module.schema.json",
  "module_id": "allisonorg:gotest",
  "visibility": "private",
  "url": "https://github.com/allisonschiang/gotest",
  "description": "Modular arm component: arm",
  "applications": null,
  "markdown_link": "README.md",
  "entrypoint": "bin/gotest",
  "first_run": "",
  "build": {
    "setup": "go mod tidy",
    "build": "go build -tags no_cgo -o bin/gotest cmd/module/main.go && tar czf module.tar.gz meta.json bin/gotest",
    "path": "module.tar.gz",
    "arch": [
      "linux/amd64",
      "linux/arm64",
      "darwin/arm64",
      "windows/amd64"
    ]
  }
}
